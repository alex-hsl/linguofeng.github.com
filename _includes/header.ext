<header class="jumbotron subhead" id="overview">
  {% if page.title %}
  <h1>{{ page.title }}</h1>
  {% endif %}
  {% if page.description %}
  <p class="lead">{{ page.description }}</p>
  {% endif %}
  {% if page.navigation %}
  {% assign navs = page.navigation %}
  <div class="subnav">
    <ul class="nav nav-pills">
    {% for nav in navs %}
      <li><a href="#{{ forloop.index }}">{{ nav }}</a></li>
    {% endfor %}
    </ul>
  </div>
  {% endif %}
</header>

<hr />